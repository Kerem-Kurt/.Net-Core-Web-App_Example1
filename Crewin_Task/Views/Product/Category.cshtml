@model List<Crewin_Task.Models.Product>

<h1>Ürünler</h1>
<ul>
    @foreach (var product in Model)
    {
        <li>
            <h2>@product.Title</h2>
            <p>@product.Description</p>
            <p>@product.Price</p>
            <img src="@product.Thumbnail" alt="@product.Title" />
        </li>
    }
</ul>
